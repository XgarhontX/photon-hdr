// ICtCp ///////////////////////////////////////////////////////////////////////////
const mat3 XYZ_TO_LMS_MAT = mat3(
    0.3592832590121217, -0.1920808463704993, 0.0070797844607479,
    0.6976051147779502, 1.1004767970374321, 0.0748396662186362,
    -0.0358915932320290, 0.0753748658519118, 0.8433265453898765);

const mat3 LMS_TO_XYZ_MAT = mat3(
    2.0701800566956132, 0.36498825003265756, -0.04959554223893212,
    -1.3264568761030210, 0.68046736285223520, -0.04942116118675749,
    0.20661600684785517, -0.045421753075853236, 1.1879959417328034);

vec3 IctcpFromBT709(vec3 bt709_color) {
  vec3 xyz_color = BT709_TO_XYZ_MAT * bt709_color;
  vec3 lms_color = XYZ_TO_LMS_MAT * xyz_color;

  // mat3 mat = mat3(
  //     0.5000, 0.5000, 0.0000,
  //     1.6137, -3.3234, 1.7097,
  //     4.3780, -4.2455, -0.1325);
  const mat3 ICTCP_LMS_TO_ICTCP_MAT = mat3(
      0.5000, 1.6137, 4.3780,
      0.5000, -3.3234, -4.2455,
      0.0000, 1.7097, -0.1325);

  return ICTCP_LMS_TO_ICTCP_MAT * PqEncode(lms_color, 100.0f);
}

vec3 BT709FromICtCp(vec3 col) {
  // mat3 mat = mat3(1.0, 0.00860514569398152, 0.11103560447547328, 1.0,
  //                 -0.00860514569398152, -0.11103560447547328, 1.0,
  //                 0.56004885956263900, -0.32063747023212210);
  const mat3 ICTCP_TO_LMS_MAT = mat3(
      1.0, 1.0, 1.0,
      0.008605146, -0.008605146, 0.56004886,
      0.11103560, -0.11103560, -0.32063747);
  col = ICTCP_TO_LMS_MAT * col;

  // 1.0f = 100 nits, 100.0f = 10k nits
  col = PqDecode(col, 100.f);
  col = LMS_TO_XYZ_MAT * col;
  return XYZ_TO_BT709_MAT * col;
}

// Hue ///////////////////////////////////////////////////////////////////////////
vec3 CorrectHue(vec3 incorrect_color, vec3 correct_color, float strength) {
  if(strength == 0.) return incorrect_color;

  vec3 incorrect_lab = IctcpFromBT709(incorrect_color);
  vec3 correct_lab = IctcpFromBT709(correct_color);

  vec2 incorrect_ab = incorrect_lab.yz;
  vec2 correct_ab = correct_lab.yz;

  // Preserve original chrominance (magnitude of the ab vector)
  float chrominance_pre_adjust = length(incorrect_ab);

  // Blend chrominance and hue by interpolating (a, b) components
  vec2 blended_ab = mix(incorrect_ab, correct_ab, strength);

  // Rescale to original chrominance to avoid saturation shift
  float chrominance_post_adjust = length(blended_ab);
  blended_ab *= DivideSafe(chrominance_pre_adjust, chrominance_post_adjust, 1.);

  incorrect_lab.yz = blended_ab;

  vec3 result = BT709FromICtCp(incorrect_lab);
  return result;
}


// Luma SetChrominance ///////////////////////////////////////////////////////////////////////////

//https://github.com/Filoppi/Luma-Framework/blob/main/Shaders/Includes/Color.hlsl
//approx colorspace agnostic saturation
vec3 Luma_SetChrominance(vec3 color, float chrominance)
{
    float maxVal = max(color.x, max(color.y, color.z));
    float minVal = min(color.x, min(color.y, color.z));
    float midVal = mix(minVal, maxVal, 0.5);
    return mix(vec3(midVal), color, vec3(chrominance));
}

// UpgradeToneMap ///////////////////////////////////////////////////////////////////////////
vec3 UpgradeToneMapBT709(
  vec3 color_untonemapped,
  vec3 color_tonemapped,
  vec3 color_tonemapped_graded,
  float post_process_strength,
  float auto_correction
) {
  float ratio = 1.;

  float y_untonemapped = YFromBT709(color_untonemapped);
  float y_tonemapped = YFromBT709(color_tonemapped);
  float y_tonemapped_graded = YFromBT709(color_tonemapped_graded);

  if(y_untonemapped < y_tonemapped) {
    // If substracting (user contrast or paperwhite) scale down instead
    // Should only apply on mismatched HDR
    ratio = y_untonemapped / y_tonemapped;
  } else {
    float y_delta = y_untonemapped - y_tonemapped;
    y_delta = max(0, y_delta);  // Cleans up NaN
    float y_new = y_tonemapped_graded + y_delta;

    bool y_valid = (y_tonemapped_graded > 0);  // Cleans up NaN and ignore black
    ratio = y_valid ? (y_new / y_tonemapped_graded) : 0;
  }
  float auto_correct_ratio = mix(1., ratio, clamp(y_untonemapped, 0, 1));
  ratio = mix(ratio, auto_correct_ratio, auto_correction);

  vec3 color_scaled = color_tonemapped_graded * ratio;
  // Match hue
  color_scaled = CorrectHue(color_scaled, color_tonemapped_graded, 1);
  return mix(color_untonemapped, color_scaled, post_process_strength);
}

// Working Color Space ////////////////////////////////////////////////////////////////

vec3 ColorSpaceConversion(vec3 x, int from, int to) {
  //BT709 to BT2020
  if(from == RENODX_CS_BT709 && to == RENODX_CS_BT2020) {
    x = BT709_TO_BT2020_MAT * x;
  }
  //BT709 to AP1
  else if(from == RENODX_CS_BT709 && to == RENODX_CS_AP1) {
    x = BT709_TO_AP1_MAT * x;
  }

  //BT2020 to BT709
  else if(from == RENODX_CS_BT2020 && to == RENODX_CS_BT709) {
    x = BT2020_TO_BT709_MAT * x;
  }
  //BT2020 to AP1
  else if(from == RENODX_CS_BT2020 && to == RENODX_CS_AP1) {
    x = BT2020_TO_AP1_MAT * x;
  }

  //AP1 to BT709
  else if(from == RENODX_CS_AP1 && to == RENODX_CS_BT709) {
    x = AP1_TO_BT709_MAT * x;
  }
  //AP1 to BT2020 
  else if(from == RENODX_CS_AP1 && to == RENODX_CS_BT2020) {
    x = AP1_TO_BT2020_MAT * x;
  }
  
  //noop
  return x;
}

vec3 WorkingColorSpace_Shaderpack_To_HDRTonemap(vec3 x) {
  return ColorSpaceConversion(x, RENODX_WORKINGCS_SHADERPACK, RENODX_WORKINGCS_HDRTONEMAP);
}

vec3 WorkingColorSpace_HDRTonemap_To_AfterTonemap(vec3 x) {
  return ColorSpaceConversion(x, RENODX_WORKINGCS_HDRTONEMAP, RENODX_WORKINGCS_AFTERTONEMAP);
}

vec3 WorkingColorSpace_RenderIntermediatePassInput_To_BT709(vec3 x) {
  return ColorSpaceConversion(x, RENODX_WORKINGCS_RENDERINTERMEDIATEPASSINPUT, RENODX_CS_BT709);
}

// Working Gamma //////////////////////////////////////////////////////////////////

vec3 WorkingGamma_Correction(vec3 x) {
  #if RENODX_GAMMACORRECTION == RENODX_GAMMA_22
    x = GammeCorrectSafe_SrgbToPow(x, 2.2);
  #elif RENODX_GAMMACORRECTION == RENODX_GAMMA_24
    x = GammeCorrectSafe_SrgbToPow(x, 2.4);
  #endif
  return x;
}

vec3 WorkingGamma_Encode(vec3 x, int target) {
  if (target == RENODX_GAMMA_20) {
    vec3 signX = Sign(x);
    vec3 absX = abs(x);
    x = signX * sqrt(absX);
  }
  else if (target == RENODX_GAMMA_22) {
    x = GammaEncodeSafe(x, 2.2);
  }
  else if (target == RENODX_GAMMA_24) {
    x = GammaEncodeSafe(x, 2.4);
  }
  else if (target == RENODX_GAMMA_SRGB) {
    x = SrgbEncodeSafe(x);
  }
  return x;
}

vec3 WorkingGamma_Decode(vec3 x, int target) {
  if (target == RENODX_GAMMA_20) {
    vec3 signX = Sign(x);
    vec3 absX = abs(x);
    x = signX * (absX * absX);
  }
  else if (target == RENODX_GAMMA_22) {
    x = GammaDecodeSafe(x, 2.2);
  }
  else if (target == RENODX_GAMMA_24) {
    x = GammaDecodeSafe(x, 2.4);
  }
  else if (target == RENODX_GAMMA_SRGB) {
    x = SrgbDecodeSafe(x);
  }
  return x;
}

vec3 WorkingGamma_AfterTonemap_Encode(vec3 x) {
  return WorkingGamma_Encode(x, RENODX_WORKINGGAMMA_AFTERTONEMAP);
}

vec3 WorkingGamma_RenderIntermediatePass_Decode(vec3 x) {
  return WorkingGamma_Decode(x, RENODX_WORKINGGAMMA_RENDERINTERMEDIATEPASSINPUT);
}

// ColorGrade ///////////////////////////////////////////////////////////////////////////
//from RenoDX clshortfuse
float RenoDX_Contrast(float x, float contrast, float mid_gray) {
  return pow(max(0, x / mid_gray), contrast) * mid_gray;
}

float RenoDX_Shadows(float x, float shadows, float mid_gray) {
  float value;
  if (shadows > 1.f) {
    value = max(x, x * (1.f + (x * mid_gray / pow(x / mid_gray, shadows))));
  } else if (shadows < 1.f) {
    value = clamp(x * (1.f - (x * mid_gray / pow(x / mid_gray, 2.f - shadows))), 0.f , x);
  } else {
    value = x;
  }
  return value;
}
float RenoDX_Highlights(float x, float highlights, float mid_gray) {
  float value;
  if (highlights > 1.f) {
    value = max(x, mix(x, mid_gray * pow(x / mid_gray, highlights), x));
  } else if (highlights < 1.f) {
    value = min(x, x / (1.f + mid_gray * pow(x / mid_gray, 2.f - highlights) - x));
  } else {
    value = x;
  }
  return value;
}

vec3 ColorGradePass(vec3 color) {
  #if RENODX_COLORGRADE_ENABLED 
    float y = YFromCS(color, RENODX_WORKINGCS_SHADERPACK);
    if (y > 0) {
      float y1 = y;

      #if RENODX_COLORGRADE_CONTRAST != 100
        y1 = RenoDX_Contrast(y1, RENODX_COLORGRADE_CONTRAST / 100., RENODX_COLORGRADE_CONTRAST_MID);
      #endif

      #if RENODX_COLORGRADE_HIGHLIGHTS != 100
        y1 = RenoDX_Highlights(y1, RENODX_COLORGRADE_HIGHLIGHTS / 100., RENODX_COLORGRADE_HIGHLIGHTS_MID);
      #endif
      
      #if RENODX_COLORGRADE_SHADOWS != 100
        y1 = RenoDX_Shadows(y1, RENODX_COLORGRADE_SHADOWS / 100., RENODX_COLORGRADE_SHADOWS_MID);
      #endif

      color *= y1/y;
    }
  #endif

  #if RENODX_COLORGRADE_SATURATION != 100
    color = Luma_SetChrominance(color, RENODX_COLORGRADE_SATURATION / 100.);
  #endif

  return color;
}

// RCAS ///////////////////////////////////////////////////////////////////////////

//https://github.com/Filoppi/Luma-Framework/blob/main/Shaders/Includes/RCAS.hlsl 

// RCAS (Robust Contrast-Adaptive Sharpening) 
// https://github.com/GPUOpen-Effects/FidelityFX-FSR2?tab=readme-ov-file#robust-contrast-adaptive-sharpening-rcas
// 
// Our implementation is based on: "https://github.com/cdozdil/OptiScaler/blob/master/OptiScaler/shaders/rcas/precompile/rcas.hlsl",
// which is in turn based on: "https://github.com/RdenBlaauwen/RCAS-for-ReShade",
// which is in turn based on AMD FSR 1 RCAS: https://github.com/GPUOpen-Effects/FidelityFX-FSR/blob/master/ffx-fsr/ffx_fsr1.h

// Try to detect noise or grain and not over sharpen it. This seemengly assumes the input is in the SDR 0-1 range.
#ifdef RENODX_RCAS_DENOISE
  #define RCAS_DENOISE 1
#else
  #define RCAS_DENOISE 0
#endif
// Lower means less artifacts and "less sharpening", it's unclear what 16 stands for here, and whether this is meant for linear or gamma space (probably gamma space)
// This is set at the limit of providing unnatural results for sharpening (anything more generates artifacts).
#ifndef RCAS_LIMIT
#define RCAS_LIMIT (0.25-(1.0/16.0))
#endif
// This should look better, avoid hue shifts and be more compatible with HDR (scRGB, which can have negative values). This appears to have stronger sharpening when enabled, but also causes more black dots to appear at extreme sharpening values.
// For now it's disabled by default as there's not enough proof to justify it, sharpening is a perceptual trick so hue shifts don't really matter (in fact, possibly they make it better).
#define RCAS_LUMINANCE_BASED 0

float RCASRenoDX_getRCASLuma(vec3 rgb) //(named weird to avoid conflict w/ shaderpack)
{
#if 0 // LUMA FT: changed to use the proper Rec.709 luminance formula (but multiplied by 2 as the sum of the original RCAS formula is 2)
    return GetLuminance(rgb) * 2;
#else
    return dot(rgb, vec3(0.5, 1.0, 0.5));
#endif
}

vec3 RCASRenoDX(sampler2D colorTex, ivec2 uv , float sharpness, float paperWhite, int renodx_gamma, int renodx_cs)
{
    if (sharpness > 1.f) sharpness /= 100.f; //assume mistake and scale down
    
    // RCAS is always "pixel based" (the next 4 pixels)
    //    b
    //  d e f
    //    h
    // We check for "maxPixelCoord" and "minPixelCoord" to support dynamic resolution scaling. We assume "pixelCoord" is already within the limits.
    vec3 e = WorkingGamma_Encode(texelFetch(colorTex, uv + ivec2( 0, 0), 0).rgb, renodx_gamma);
    if (sharpness == 0.0f) return e;
    e /= paperWhite;
    vec3 b = WorkingGamma_Encode(texelFetch(colorTex, uv + ivec2( 0,-1), 0).rgb, renodx_gamma) / paperWhite;
    vec3 d = WorkingGamma_Encode(texelFetch(colorTex, uv + ivec2(-1, 0), 0).rgb, renodx_gamma) / paperWhite;
    vec3 f = WorkingGamma_Encode(texelFetch(colorTex, uv + ivec2( 1, 0), 0).rgb, renodx_gamma) / paperWhite;
    vec3 h = WorkingGamma_Encode(texelFetch(colorTex, uv + ivec2( 0, 1), 0).rgb, renodx_gamma) / paperWhite;

#if RCAS_DENOISE >= 1
    // Get lumas times 2. Should use luma weights that are twice as large as normal.
    float bL = RCASRenoDX_getRCASLuma(b);
    float dL = RCASRenoDX_getRCASLuma(d);
    float eL = RCASRenoDX_getRCASLuma(e);
    float fL = RCASRenoDX_getRCASLuma(f);
    float hL = RCASRenoDX_getRCASLuma(h);

    // denoise
    float nz = (bL + dL + fL + hL) * 0.25 - eL;
    float range = max(max(max(bL, dL), max(hL, fL)), eL) - min(min(min(bL, dL), min(eL, fL)), hL);
    nz = clamp(abs(nz) * (1/(range)),0,1);
    nz = -0.5 * nz + 1.0;
#endif

    const float samplesNum = 4.0; // There's 4 (5) colors to be mixed
    // Immediate constants for peak range.
    const vec2 peakC = vec2(1.0, -samplesNum);

#if RCAS_LUMINANCE_BASED
    // These should all be >= 0, but tiny values below zero shouldn't hurt anyway, still, we clip them below as they'd be clipped by the screen anyway
    float bLum = YFromCS(b, renodx_cs);
    float dLum = YFromCS(d, renodx_cs);
    float eLum = YFromCS(e, renodx_cs);
    float fLum = YFromCS(f, renodx_cs);
    float hLum = YFromCS(h, renodx_cs);

    float minLum = max(min(min(bLum, dLum), min(fLum, hLum)), 0.0);
    float maxLum = max(max(bLum, dLum), max(fLum, hLum));

    float hitMin = minLum * (1/(samplesNum * maxLum));
    float hitMax = (peakC.x - maxLum) * (1/(samplesNum * minLum + peakC.y));

    float localLobe = max(-hitMin, hitMax);
#else // !RCAS_LUMINANCE_BASED
    // Min and max of ring.
    vec3 minRGB = min(min(b, d), min(f, h));
    vec3 maxRGB = max(max(b, d), max(f, h));

#if 0 // It seems like it's ok if these values aren't in the 0-1 range, the code below will still work as expected, and avoid clipping HDR (or anyway ignoring values beyond SDR) //TODOFT3: Lilium claims the code below can break for negative rgb (scRGB) values, investigate and fix it
    minRGB = clamp(minRGB, 0, 1);
    maxRGB = clamp(maxRGB, 0, 1);
#endif

    // Limiters, these need to use high precision reciprocal operations.
    // Decided to use standard rcp for now in hopes of optimizing it.
    // It's fine if either of these can go below zero!
    vec3 hitMin = minRGB * (1/(samplesNum * maxRGB));
    vec3 hitMax = (peakC.xxx - maxRGB) * (1/(samplesNum * minRGB + peakC.yyy));

    vec3 lobeRGB = max(-hitMin, hitMax);
#if 0 // An attempt to make this code, which branches by r g b channel, color space agnostic. Without this, the result heavily depends by where the rgb coordinates are in the CIE color graph, and by how luminous they are. Unfortunately this drastically reduces the sharpening intensity, even if it makes it look even more natural.
    float localLobe = max(lobeRGB.r * Rec709_Luminance.r * 3.0, max(lobeRGB.g * Rec709_Luminance.g * 3.0, lobeRGB.b * Rec709_Luminance.b * 3.0));
#else
    float localLobe = max(lobeRGB.r, max(lobeRGB.g, lobeRGB.b));
#endif
#endif // RCAS_LUMINANCE_BASED

    float lobe = max(-RCAS_LIMIT, min(localLobe, 0.0)) * sharpness;

#if RCAS_DENOISE >= 1
    // denoise
    lobe *= nz;
#endif

    // Resolve, which needs medium precision rcp approximation to avoid visible tonality changes.
    float rcpL = (1/(samplesNum * lobe + 1.0));
    
#if RCAS_LUMINANCE_BASED
    float outputLum = ((bLum + dLum + hLum + fLum) * lobe + eLum) * rcpL;
    // Questionable choice: in case the source center pixel had luminance zero (even if the rgb ratio wasn't flat), elevate it to grey and match the target luminance, or we'd have a division by zero.
    // The alternative would be to keep "e" intact, but that then wouldn't have applied any sharpening.
    vec3 output1 = eLum != 0 ? (e * (outputLum / eLum)) : outputLum;
#else // !RCAS_LUMINANCE_BASED
    vec3 output1 = ((b + d + f + h) * lobe + e) * rcpL;
#endif // RCAS_LUMINANCE_BASED

#if 0 // Debug
    if (dynamicSharpening)
    {
        if (originalSharpness < sharpness)
            output.r *= 1 + (12.0 * (sharpness - originalSharpness));
        else
            output.g *= 1 + (12.0 * (originalSharpness - sharpness));
    }
#endif

    output1 *= paperWhite;

    return output1;
}

#endif // SRC_RCAS_HLSL

// Apply Per Channel Correction ////////////////////////////////////////////////////////
struct ApplyPerChannelCorrectionResult {
  vec3 corrected_color;
  float tonemapped_luminance;
};
ApplyPerChannelCorrectionResult ApplyPerChannelCorrection_Internal(
    vec3 untonemapped,
    vec3 per_channel_color,
    float blowout_restoration,
    float hue_correction_strength,
    float chrominance_correction_strength,
    float hue_shift_strength) {
  ApplyPerChannelCorrectionResult result;

  float tonemapped_luminance = YFromBT709(abs(per_channel_color));
  result.tonemapped_luminance = tonemapped_luminance;

  const float AUTO_CORRECT_BLACK = 0.02;
  // Fix near black
  float untonemapped_luminance = YFromBT709(abs(untonemapped));
  float ratio = tonemapped_luminance / untonemapped_luminance;
  float auto_correct_ratio = mix(ratio, 1.0,
                                 clamp(untonemapped_luminance / AUTO_CORRECT_BLACK, 0.0, 1.0));
  untonemapped *= auto_correct_ratio;

  vec3 tonemapped_perceptual = IctcpFromBT709(per_channel_color);
  vec3 untonemapped_perceptual = IctcpFromBT709(untonemapped);

  vec2 untonemapped_chromas = untonemapped_perceptual.yz;
  vec2 tonemapped_chromas = tonemapped_perceptual.yz;

  float untonemapped_chrominance = length(untonemapped_perceptual.yz);  // eg: 0.80
  float tonemapped_chrominance = length(tonemapped_perceptual.yz);      // eg: 0.20

  // clamp saturation loss
  float chrominance_ratio = min(DivideSafe(tonemapped_chrominance,
                                           untonemapped_chrominance, 1.0),
                                1.0);
  chrominance_ratio = max(chrominance_ratio, blowout_restoration);

  // Untonemapped hue, tonemapped chrominance (with limit)
  vec2 reduced_untonemapped_chromas = untonemapped_chromas * chrominance_ratio;

  // pick chroma based on per-channel luminance (supports not oversaturating crushed areas)
  vec2 reduced_hue_shifted = mix(
      tonemapped_chromas,
      reduced_untonemapped_chromas,
      clamp(tonemapped_luminance / 0.36, 0.0, 1.0));

  // Tonemapped hue, restored chrominance (with limit)
  vec2 blowout_restored_chromas = tonemapped_chromas * DivideSafe(length(reduced_hue_shifted), length(tonemapped_chromas), 1.0);

  vec2 hue_shifted_chromas = mix(reduced_hue_shifted,
                                       blowout_restored_chromas,
                                       hue_shift_strength);

  // Pick untonemapped hues for shadows/midtones
  vec2 hue_correct_chromas = untonemapped_chromas * DivideSafe(length(hue_shifted_chromas), length(untonemapped_chromas), 1.0);

  vec2 selectable_hue_correct_range = mix(
      hue_correct_chromas,
      hue_shifted_chromas,
      clamp(tonemapped_luminance / 0.36, 0.0, 1.0));

  vec2 hue_corrected_chromas = mix(hue_shifted_chromas,
                                         selectable_hue_correct_range,
                                         hue_correction_strength);

  vec2 chroma_correct_chromas = hue_corrected_chromas * DivideSafe(length(untonemapped_chromas), length(hue_corrected_chromas), 1.0);

  vec2 selectable_chroma_correct_range = mix(
      chroma_correct_chromas,
      hue_corrected_chromas,
      clamp(tonemapped_luminance / 0.36, 0.0, 1.0));

  vec2 chroma_corrected_chromas = mix(
      hue_correct_chromas,
      selectable_chroma_correct_range,
      chrominance_correction_strength);

  vec2 final_chromas = chroma_corrected_chromas;

  result.corrected_color = BT709FromICtCp(vec3(
      tonemapped_perceptual.x,
      final_chromas));

  return result;
}

//For UpgradeToneMap() pairing. 
// Run me right after SDT tonemap to restore lost chroma from quick SDR blowouts.
// Requires both BT709.
vec3 ApplyPerChannelCorrection(vec3 untonemapped, vec3 per_channel_color) {
#if RENODX_APPC_AMOUNT == 0
  return per_channel_color;
#else
  ApplyPerChannelCorrectionResult result = ApplyPerChannelCorrection_Internal(
      untonemapped,
      per_channel_color,
      RENODX_APPC_AMOUNT / 100.,
      1.0,
      1.0,
      0.6);

  result.corrected_color = max(vec3(0), result.corrected_color);
  result.corrected_color = ClampByMaxScaling(result.corrected_color, 1);

  #if RENODX_APPC_POW > 0
    float strength = result.tonemapped_luminance;
    strength = clamp(strength, 0.0, 1.0);
    strength = pow(strength, RENODX_APPC_POW);
    return mix(per_channel_color, result.corrected_color, strength);
  #else
    return result.corrected_color;
  #endif
#endif
}


// Reinhard ///////////////////////////////////////////////////////////////////////////
vec3 ReinhardInv(vec3 x) {
  return x / (1.0 - x);
}
vec3 ReinhardInv(vec3 x, float peak) {
  return x / (1.0 - x / peak);
}
float ReinhardInv(float x) {
  return x / (1.0 - x);
}
float ReinhardInv(float x, float peak) {
  return x / (1.0 - x / peak);
}

vec3 Reinhard(vec3 x) {
  return x / (x + 1.0);
}
vec3 Reinhard(vec3 x, float peak) {
  return x / (x / peak + 1.);
}
vec3 ReinhardExtended(vec3 color, float white_max, float peak) {
  return Reinhard(color, peak) * (1. + (peak * color) / (white_max * white_max));
}

float Reinhard(float x, float peak) {
  return x / (x / peak + 1.);
}
float ReinhardExtended(float color, float white_max, float peak) {
  return Reinhard(color, peak) * (1. + (peak * color) / (white_max * white_max));
}

float ComputeReinhardExtendableScale(float w, float p, float m, float x, float y) {
  return p * (w * w * y - (p * x * x)) / (w * w * x * (p - y));
}

